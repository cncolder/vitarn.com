{
    "disallowSpacesInFunction": null,
    "esnext": true,
    "excludeFiles": [
        "node_modules/**",
        "*.min.js"
    ],
    "maximumLineLength": {
        "allowComments": true,
        "value": 80
    },
    "preset": "node-style-guide",
    "requireCamelCaseOrUpperCaseIdentifiers": "ignoreProperties",
    "requireCapitalizedComments": null,
    "requireCurlyBraces": [
        "if",
        "else",
        "for",
        "while",
        "do",
        "try",
        "catch"
    ]
}
